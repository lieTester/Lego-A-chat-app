{
   "version": 2,
   "routes": [
      {
         "src": "/api/.*",
         "dest": "index.js"
      },
      {
         "src": "/(.*)",
         "dest": "index.js"
      }
   ],
   "builds": [
      {
         "src": "package.json",
         "use": "@vercel/node"
      }
   ],
   "env": {
      "NODE_ENV": "production"
   }
}
